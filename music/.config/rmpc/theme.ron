#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    default_album_art_path: None,
    show_song_table_header: true,
    draw_borders: true,
    format_tag_separator: " | ",
    browser_column_widths: [20, 38, 42],
    background_color: "#110011",
    text_color: "#FFF9CC",
    header_background_color: "#110011",
    modal_background_color: "#110011",
    modal_backdrop: false,
    preview_label_style: (fg: "#B9ADDF"),
    preview_metadata_group_style: (fg: "#B9ADDF", modifiers: "Bold"),
    tab_bar: (
        active_style: (fg: "#110011", bg: "#B9ADDF", modifiers: "Bold"),
        inactive_style: (fg: "#ADA779"),
    ),
    highlighted_item_style: (fg: "#B9ADDF", modifiers: "Bold"),
    current_item_style: (fg: "#110011", bg: "#B9ADDF", modifiers: "Bold"),
    borders_style: (fg: "#ADA779"),
    highlight_border_style: (fg: "#B9ADDF"),
    symbols: (
        song: "󰎆",
        dir: "",
        playlist: "󰲸",
        marker: "",
        ellipsis: "...",
        song_style: (fg: "#B9ADDF"),
        dir_style: (fg: "#B9ADDF"),
        playlist_style: (fg: "#FA4D29"),
    ),
    level_styles: (
        info: (fg: "#B9ADDF", bg: "#110011"),
        warn: (fg: "#AD9882", bg: "#110011"),
        error: (fg: "#477170", bg: "#110011"),
        debug: (fg: "#FA4D29", bg: "#110011"),
        trace: (fg: "#16D0CF", bg: "#110011"),
    ),
    progress_bar: (
        symbols: ["", "━", "╸", "─", ""],
        track_style: (fg: "#ADA779"),
        elapsed_style: (fg: "#B9ADDF"),
        thumb_style: (fg: "#B9ADDF", bg: "#ADA779"),
        use_track_when_empty: false,
    ),
    scrollbar: (
        symbols: ["│", "█", "▲", "▼"],
        track_style: (fg: "#ADA779"),
        ends_style: (fg: "#ADA779"),
        thumb_style: (fg: "#B9ADDF"),
    ),
    song_table_format: [
        (
            prop: (kind: Property(Artist),
                default: (kind: Text("Unknown"))
            ),
            width: "20%",
        ),
        (
            prop: (kind: Property(Title),
                default: (kind: Text("Unknown"))
            ),
            width: "35%",
        ),
        (
            prop: (kind: Property(Album), style: (fg: "#ADA779"),
                default: (kind: Text("Unknown Album"), style: (fg: "#ADA779"))
            ),
            width: "30%",
        ),
        (
            prop: (kind: Property(Duration),
                default: (kind: Text("-"))
            ),
            width: "15%",
            alignment: Right,
        ),
    ],
    components: {},
    layout: Split(
        direction: Vertical,
        panes: [
            (
                pane: Pane(Header),
                size: "2",
            ),
            (
                pane: Pane(Tabs),
                size: "3",
            ),
            (
                pane: Pane(TabContent),
                size: "100%",
            ),
            (
                pane: Pane(ProgressBar),
                size: "1",
            ),
        ],
    ),
    header: (
        rows: [
            (
                left: [
                    (kind: Text("["), style: (fg: "#B9ADDF", modifiers: "Bold")),
                    (kind: Property(Status(StateV2(playing_label: "Playing", paused_label: "Paused", stopped_label: "Stopped"))), style: (fg: "#B9ADDF", modifiers: "Bold")),
                    (kind: Text("]"), style: (fg: "#B9ADDF", modifiers: "Bold"))
                ],
                center: [
                    (kind: Property(Song(Title)), style: (fg: "#FFF9CC", modifiers: "Bold"),
                        default: (kind: Text("No Song"), style: (fg: "#ADA779", modifiers: "Bold"))
                    )
                ],
                right: [
                    (kind: Property(Widget(ScanStatus)), style: (fg: "#B9ADDF")),
                    (kind: Property(Widget(Volume)), style: (fg: "#B9ADDF"))
                ]
            ),
            (
                left: [
                    (kind: Property(Status(Elapsed)), style: (fg: "#ADA779")),
                    (kind: Text(" / "), style: (fg: "#ADA779")),
                    (kind: Property(Status(Duration)), style: (fg: "#ADA779")),
                    (kind: Text(" ("), style: (fg: "#ADA779")),
                    (kind: Property(Status(Bitrate)), style: (fg: "#ADA779")),
                    (kind: Text(" kbps)"), style: (fg: "#ADA779"))
                ],
                center: [
                    (kind: Property(Song(Artist)), style: (fg: "#B9ADDF", modifiers: "Bold"),
                        default: (kind: Text("Unknown"), style: (fg: "#B9ADDF", modifiers: "Bold"))
                    ),
                    (kind: Text(" - "), style: (fg: "#ADA779")),
                    (kind: Property(Song(Album)), style: (fg: "#ADA779"),
                        default: (kind: Text("Unknown Album"), style: (fg: "#ADA779"))
                    )
                ],
                right: [
                    (
                        kind: Property(Widget(States(
                            active_style: (fg: "#B9ADDF", modifiers: "Bold"),
                            separator_style: (fg: "#ADA779")))
                        ),
                        style: (fg: "#ADA779")
                    ),
                ]
            ),
        ],
    ),
    browser_song_format: [
        (
            kind: Group([
                (kind: Property(Track)),
                (kind: Text(" ")),
            ])
        ),
        (
            kind: Property(Title),
            default: (kind: Property(Filename))
        ),
    ],
    lyrics: (
        timestamp: false
    )
)
